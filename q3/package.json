{
    "name": "Q3 (Syntax Highlighting)",
    "displayName": "Q3 (Syntax Highlighting)",
    "description": "Syntax highlighting for Q3 and nothing else.",
    "version": "1.0.0",
    "icon":"logo.png",
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "snippets": [
            {
              "language": "q3",
              "path": "./snippets/q3-snippets.json"
            }
          ],
        "themes": [ 
            {
                "label": "q3",
                "uiTheme": "vs-dark",
                "path": "./themes/q3-color-theme.json"
            }
        ],
        "languages": [{
            "id": "q3",
            "aliases": ["q3", "q3"],
            "extensions": ["q3"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "q3",
            "scopeName": "source.q3",
            "path": "./syntaxes/q3.tmLanguage.json"
        }]
    }
}